{
  "name": "dnadesign/silverstripe-embed",
  "description": "Adds Embed and Video a DataObjects along with a DataExtension to apply embed capabilities to existing objects.",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Gorrie Coe",
      "email": "gorriecoe@gmail.com"
    },
    {
      "name": "DNA Designed Communications Limited",
      "homepage": "https://www.dna.co.nz"
    }
  ],
  "keywords": [
    "silverstripe",
    "cms",
    "embed"
  ],
  "homepage": "http://github.com/dnadesign/silverstripe-embed",
  "require": {
    "silverstripe/framework": "^4.11 || ^5",
    "gorriecoe/silverstripe-htmltag": "dev-master",
    "embed/embed": "^4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "gorriecoe\\Embed\\": "src/",
      "gorriecoe\\Embed\\Test\\": "tests/"
    }
  },
  "replace": {
    "gorriecoe/silverstripe-embed": "*"
  }
}
